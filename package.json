{
  "name": "enchantify",
  "version": "0.0.0-semantic-release",
  "description": "Enchantify Discord Bot",
  "main": "src/index.ts",
  "author": "Cameron Shatto <cameronshatto99@gmail.com>",
  "license": "MIT",
  "private": true,
  "homepage": "https://github.com/CShatto99/Enchantify/blob/main/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/CShatto99/Enchantify"
  },
  "keywords": [
    "typescript",
    "discord.js"
  ],
  "bugs": {
    "url": "https://github.com/CShatto99/Enchantify/issues"
  },
  "scripts": {
    "start": "yarn build && node dist/index.js",
    "build": "npx tsc",
    "dev": "nodemon",
    "migrate": "prisma migrate dev --name init",
    "type-check": "tsc --pretty --noEmit",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "pretty": "prettier --config .prettierrc src/**/*.ts --write",
    "test": "jest --roots test --verbose --watch",
    "test:ci": "jest --roots test --ci",
    "test:coverage": "jest --roots test --verbose --collect-coverage",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "discord.js": "^14.8.0",
    "dotenv": "^16.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@types/dotenv": "^8.2.0",
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.8.4"
  }
}
