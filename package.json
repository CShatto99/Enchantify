{
  "name": "enchantify",
  "version": "0.0.0-semantic-release",
  "description": "Enchantify Discord Bot",
  "main": "src/index.ts",
  "author": "Cameron Shatto <cameronshatto99@gmail.com>",
  "license": "MIT",
  "private": true,
  "homepage": "https://github.com/CShatto99/Enchantify/blob/main/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/CShatto99/Enchantify"
  },
  "keywords": [
    "typescript",
    "discord.js"
  ],
  "bugs": {
    "url": "https://github.com/CShatto99/Enchantify/issues"
  },
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "dev": "nodemon",
    "type-check": "tsc --pretty --noEmit",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "pretty": "prettier --config .prettierrc src/**/*.ts --write",
    "test": "jest --roots test --verbose --watch",
    "test:ci": "jest --roots test --ci",
    "test:coverage": "jest --roots test --verbose --collect-coverage",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.2",
    "mongodb": "^6.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
